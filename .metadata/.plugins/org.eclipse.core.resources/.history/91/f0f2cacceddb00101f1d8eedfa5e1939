#include <stdint.h>
#include <stm32f446xx.h>
#include "main.h"
#include "gpio.h"



uint8_t SPI1_TransmiReceive(uint8_t spiData){
	SPI1->DR = spiData;
	while(SPI1-> SR & SPI_SR_TXE_Msk);
	while(SPI1-> SR & SPI_SR_RXNE_Msk);
	return SPI1->DR;

}

